{
    "preset": "laravel",
    "rules": {
        "@PSR12": true,
        "no_unused_imports": true,
        "declare_strict_types": true,
        "phpdoc_to_param_type": true,
        "phpdoc_to_return_type": true,
        "phpdoc_align": true,
        "array_syntax": {
            "syntax": "short"
        },
        "trailing_comma_in_multiline": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        }
    }
}    
